# Copy this file to .env and fill values before running the server

# OpenAI API key for ChatGPT provider (keep secret)
# Get it at https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Serper API key for web search provider (keep secret)
# Free tier available at https://serper.dev
SERPER_API_KEY=

# Optional: override backend API URL used by the React frontend
# e.g. REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_URL=
